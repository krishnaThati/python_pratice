#list and it's methods

numbers = [1,2,3,4,5]
print(numbers)
print(numbers[2])
numbers.append(0)
print(numbers)
numbers.insert(3,-1)
print(numbers)
print(0 in numbers)
print(len(numbers)) #len a function so it is inpurple color
numbers.remove(3)
print(numbers)
numbers.clear()
print(numbers)



names = ["kc","jan","jap","sandy"]
print(names)
names[1] = "jagan"
print(names)
print(names[0:3])
